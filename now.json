{
  "version": 2,
  "name": "jolvera.dev",
  "alias": ["jolvera.dev", "jolvera.now.sh"],
  "builds": [
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "src": "/feed.json", "dest": "/static/feed.json" },
    { "src": "/blog/(?<page>[^/]+)$", "dest": "/blog?page=$page" }
  ]
}
